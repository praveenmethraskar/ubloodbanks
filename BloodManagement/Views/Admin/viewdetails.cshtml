@using Microsoft.AspNetCore.Http
@model IEnumerable<BloodManagement.Models.AddMoreDetails>
@{
    ViewData["Title"] = "viewdetails";
    ViewBag.Title = "viewdetails";
}

<!--Preloader starts-->
<div class="preloader js-preloader">

    <img src="~/css/img/preloader.gif" />
</div>
<!--Preloader ends-->
<!-- page wrapper Start -->
<div class="page-wrapper">
    <!-- Header Section Start -->
    <header class="header-wrap style2">
        <div class="header-top bg-mine-shaft">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-xl-4 col-lg-12">
                        <div class="header-top-right">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="header-bottom">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-xl-3 col-lg-6 col-md-6 col-5">
                        <a href="index.html" class="logo"><img src="~/css/img/logo-h.png" alt="Image"></a>
                    </div>
                    <div class="col-xl-6 col-lg-6 col-md-6 col-7">
                        <div class="main-menu-wrap">
                            <div class="menu-close xl-none">
                                <a href="javascript:void(0)"><i class="las la-times"></i></a>
                            </div>
                            <div id="menu">
                                <ul class="main-menu list-style">
                                    <li>
                                        <a asp-action="AdminRegistration">Add more Admins</a>
                                    </li>
                                    <li>
                                        <a asp-action="AddHospitals">Add Hospitals</a>
                                    </li>
                                    <li>
                                        <a asp-action="viewdetails">View Donors</a>
                                    </li>

                                </ul>
                            </div>
                        </div>
                        <div class="mobile-bar-wrap">

                            <div class="mobile-menu xl-none">
                                <a href="javascript:void(0)"><i class="las la-bars"></i></a>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 lg-none">
                        <div class="header-bottom-right">
                            <a asp-action="Logout" class="btn style2"><i class="flaticon-user-3"></i> Logout </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>


    <!-- Content Wrapper Start -->
    <div class="content-wrapper">
        <div class="breadcrumb-wrap bg-f breadcrumb-3">
            <div class="overlay bg-blue op-4"></div>
            <div class="container">
                <div class="row">
                    <div class="col-xl-12">
                        <div class="breadcrumb-title">
                            <h2>Welcome @Context.Session.GetString("name")</h2>
                            @*       <ul class="breadcrumb-menu list-style">
                            <li><a href="#">Dashboard </a></li>
                            <li>Register </li>
                            </ul>*@
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Breadcrumb End -->

        <section>
            <div class="container">

                <!--Table-->
                <table class="table table-striped w-auto">
                    <tr>
                        <th> @Html.DisplayNameFor(model => model.fullname)</th>
                        <th>  @Html.DisplayNameFor(model => model.mail)</th>
                        <th> @Html.DisplayNameFor(model => model.DoB)</th>
                        <th>  @Html.DisplayNameFor(model => model.Bloodgroup)</th>
                        <th> @Html.DisplayNameFor(model => model.mobile)</th>
                        <th> @Html.DisplayNameFor(model => model.address1)</th>
                        <th> @Html.DisplayNameFor(model => model.district)</th>
                        <th> @Html.DisplayNameFor(model => model.states)</th>
                        <th> @Html.DisplayNameFor(model => model.pincode)</th>
                    </tr>

                   
                        @foreach (var item in ViewBag.Model)
                        {
                        <tr>
                            <td> @item.fullname </td>
                                <td> @item.mail</td>
                                <td> @item.DoB </td>
                                <td> @item.Bloodgroup </td>
                            <td> @item.mobile </td>
                            <td> @item.address1 </td>
                            <td> @item.district </td>
                            <td> @item.states </td>
                            <td> @item.pincode </td>
                        </tr>
                        }
                    
                </table>
                <!--Table-->
            </div>
        </section>

    </div>
    <!-- Content Wrapper End -->

</div>
<!-- Page wrapper End -->
<!-- Footer Section Start -->
<footer class="footer-wrap bg-f footer-bg-1 fixed-footer">
    <div class="overlay op-96 bg-blue"></div>
    <div class="footer-top pt-0 pb-0">
        <div class="container">
            <div class="row">
                <div class="footer-widget-wrap">
                    <div class="footer-widget">
                        <a href="index.html" class="footer-logo">
                            <img src="~/css/img/logo-h.png" alt="Image">
                        </a>


                    </div>
                    <div class="footer-widget"></div>
                    <div class="footer-widget"> </div>
                    <div class="footer-widget">
                        <div class="social-box">
                            <ul class="social-profile style1 list-style" style="padding:50px">
                                <li>
                                    <a target="_blank" href="https://facebook.com/">
                                        <i class="flaticon-facebook"></i>
                                    </a>
                                </li>
                                <li>
                                    <a target="_blank" href="https://twitter.com/">
                                        <i class="flaticon-twitter-1"></i>
                                    </a>
                                </li>
                                <li>
                                    <a target="_blank" href="https://instagram.com/">
                                        <i class="flaticon-instagram-1"></i>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <hr style="border-top: 1px solid red;">
    <div class="footer-bottom">
        <div class="copyright">
            <p>© Copyrights 2020 UBlood, All rights reserved. <a href="https://envytheme.com/" target="_blank"></a></p>
        </div>
    </div>
</footer>
<!-- Footer Section End -->
<!-- Footer Section End -->
<!-- Back-to-top button Start -->
<a href="javascript:void(0)" class="back-to-top"><i class="las la-arrow-up"></i></a>
<!-- Back-to-top button End -->
<!-- Link of JS files -->
<script src="~/js/jquery.min.js"></script>
<script src="~/js/bootstrap.bundle.min.js"></script>
<script src="~/js/form-validator.min.js"></script>
<script src="~/js/contact-form-script.js"></script>
<script src="~/js/aos.js"></script>
<script src="~/js/owl.carousel.min.js"></script>
<script src="~/js/jquery-magnific-popup.js"></script>
<script src="~/js/fancybox.js"></script>
<script src="~/js/progressbar.min.js"></script>
<script src="~/js/jquery.appear.js"></script>
<script src="~/js/tweenmax.min.js"></script>
<script src="~/js/main.js"></script>


